<UserControl x:Class="SkillPlayerMover.SkillPlayerMoverGui"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:settingsPath="clr-namespace:SkillPlayerMover"
             xmlns:mahapps="http://metro.mahapps.com/winfx/xaml/controls"
             mc:Ignorable="d" 
             d:DesignHeight="Auto" d:DesignWidth="Auto">
    <Grid x:Name="Root" DataContext="{x:Static settingsPath:SkillPlayerMoverSettings.Instance}" Width="Auto" Height="Auto">
        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
            <StackPanel>
                <GroupBox Header="Basic Configuration">
                    <StackPanel>
                        <DockPanel>
                            <Label Content="MoveRange: " Margin="5,0,0,0" ToolTipService.ToolTip="When the path calculation decide what is next point to move to, it filter out point to close to us, until it reach a distance >= to this value. [25/30 are great average value]" />
                            <mahapps:NumericUpDown Margin="5,0,0,0" Value="{Binding Path=MoveRange, Mode=TwoWay}" Minimum="15" Maximum="45" HorizontalAlignment="Left"/>
                        </DockPanel>
                        <DockPanel>
                            <Label Content="SingleUseDistance: " Margin="5,0,0,0" ToolTipService.ToolTip="When the path calculation don't have points at a distance higher than this value, the single use is triggered and this mean that no skill to move is used, and the mover click the basic move skill only once,  instead of keeping the button pressed. this is the situatiion where you are approaching a npc, item, transaction or anything you want to interact to. [15/18 are best average]" />
                            <mahapps:NumericUpDown Margin="5,0,0,0" Value="{Binding Path=SingleUseDistance, Mode=TwoWay}" Minimum="15" Maximum="45" HorizontalAlignment="Left"/>
                        </DockPanel>
                    </StackPanel>
                </GroupBox>
                <Expander Header="Skills">
                    <StackPanel>
                        <DockPanel>
                            <Label Content="Min mana %:" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" ToolTip="The move skill will not be used under this percent of mana."/>
                            <TextBox Text="{Binding ElementName=Minmana, Path=Value, UpdateSourceTrigger=PropertyChanged}" Margin="5,2,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="Minmana" Value="{Binding Path=MoveMinManaValue, Mode=TwoWay}"  HorizontalAlignment="Left" Margin="5,0,0,0" Maximum="100" Minimum="0" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="The move skill will not be used under this percent of mana."/>
                            <CheckBox Content="Use Blood Magic" IsChecked="{Binding Path=UseBloodMagicValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" ToolTip="Activate if you use Blood Magic with the Move Skill."></CheckBox>
                        </DockPanel>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Label Content="Skill:" HorizontalAlignment="Left" Margin="10,5,0,0" Grid.Row="0" VerticalAlignment="Top"></Label>
                            <Label Content="Min Distance" HorizontalAlignment="Left" Margin="120,5,0,0" Grid.Row="0" VerticalAlignment="Top"></Label>
                            <Label Content="Max Distance" HorizontalAlignment="Left" Margin="250,5,0,0" Grid.Row="0" VerticalAlignment="Top"></Label>

                            <CheckBox Content="Ligtning Warp:" IsChecked="{Binding Path=EnableLwValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="1" VerticalAlignment="Top" ToolTip="Use Ligtning Warp?"></CheckBox>
                            <TextBox Text="{Binding ElementName=LigtningWarpMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="1" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="LigtningWarpMinDist" Value="{Binding Path=LwMinDistValue, Mode=TwoWay}"  HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="1" Maximum="60" Minimum="15" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>
                            <TextBox Text="{Binding ElementName=LigtningWarpMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="1" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="LigtningWarpMaxDist" Value="{Binding Path=LwMaxDistValue, Mode=TwoWay}"  HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="1" Maximum="120" Minimum="{Binding ElementName=LigtningWarpMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="Blink Arrow:" IsChecked="{Binding Path=EnableBaValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="2" VerticalAlignment="Top" ToolTip="Use Blink Arrow?"></CheckBox>
                            <TextBox Text="{Binding ElementName=BlinkArrowMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="2" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="BlinkArrowMinDist" Value="{Binding Path=BaMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="2" Maximum="60" Minimum="15" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>
                            <TextBox Text="{Binding ElementName=BlinkArrowMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="2" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="BlinkArrowMaxDist" Value="{Binding Path=BaMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="2" Maximum="120" Minimum="{Binding ElementName=BlinkArrowMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="Leap Slam:" IsChecked="{Binding Path=EnableLsValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="3" VerticalAlignment="Top" ToolTip="Use Leap Slam?"></CheckBox>
                            <TextBox Text="{Binding ElementName=LeapSlamMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="3" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="LeapSlamMinDist" Value="{Binding Path=LsMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="3" Maximum="60" Minimum="15" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>

                            <TextBox Text="{Binding ElementName=LeapSlamMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="3" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="LeapSlamMaxDist" Value="{Binding Path=LsMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="3" Maximum="120" Minimum="{Binding ElementName=LeapSlamMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}"  TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="Flame Dash:" IsChecked="{Binding Path=EnableFdValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="4" VerticalAlignment="Top" ToolTip="Use Flame Dash?"></CheckBox>
                            <TextBox Text="{Binding ElementName=FlameDashMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="4" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="FlameDashMinDist" Value="{Binding Path=FdMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="4" Maximum="60" Minimum="27" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>

                            <TextBox Text="{Binding ElementName=FlameDashMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="4" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="FlameDashMaxDist" Value="{Binding Path=FdMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="4" Maximum="120" Minimum="{Binding ElementName=FlameDashMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="Phase Run:" IsChecked="{Binding Path=EnablePrValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="5" VerticalAlignment="Top" ToolTip="Use Phase Run?"></CheckBox>
                            <TextBox Text="{Binding ElementName=PhaseRunMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="5" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="PhaseRunMinDist" Value="{Binding Path=PrMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="5" Maximum="60" Minimum="1" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>

                            <TextBox Text="{Binding ElementName=PhaseRunMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="5" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="PhaseRunMaxDist" Value="{Binding Path=PrMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="5" Maximum="120" Minimum="{Binding ElementName=PhaseRunMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="Whirling Blades:" IsChecked="{Binding Path=EnableWbValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="6" VerticalAlignment="Top" ToolTip="Use Whirling Blades?"></CheckBox>
                            <TextBox Text="{Binding ElementName=WhirlingBladesMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="6" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="WhirlingBladesMinDist" Value="{Binding Path=WbMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="6" Maximum="60" Minimum="15" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>

                            <TextBox Text="{Binding ElementName=WhirlingBladesMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="6" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="WhirlingBladesMaxDist" Value="{Binding Path=WbMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="6" Maximum="120" Minimum="{Binding ElementName=WhirlingBladesMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="Shield Charge:" IsChecked="{Binding Path=EnableScValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="7" VerticalAlignment="Top" ToolTip="Use Shiel Charge?"></CheckBox>
                            <TextBox Text="{Binding ElementName=ShieldChargeMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="7" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="ShieldChargeMinDist" Value="{Binding Path=ScMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="7" Maximum="60" Minimum="27" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>

                            <TextBox Text="{Binding ElementName=ShieldChargeMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="7" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="ShieldChargeMaxDist" Value="{Binding Path=ScMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="7" Maximum="120" Minimum="{Binding ElementName=ShieldChargeMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="Frenzy:" IsChecked="{Binding Path=EnableFrValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="8" VerticalAlignment="Top" ToolTip="Use Frenzy? Frenzy code is curtesy of fmflex, use it with PoetPen build."></CheckBox>
                            <TextBox Text="{Binding ElementName=FrenzyMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="8" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="FrenzyMinDist" Value="{Binding Path=FrMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="8" Maximum="60" Minimum="1" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>

                            <TextBox Text="{Binding ElementName=FrenzyMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="8" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="FrenzyMaxDist" Value="{Binding Path=FrMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="8" Maximum="120" Minimum="{Binding ElementName=FrenzyMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="Withering Step:" IsChecked="{Binding Path=EnableWsValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="9" VerticalAlignment="Top" ToolTip="Use Withering Step?"></CheckBox>
                            <TextBox Text="{Binding ElementName=WsMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="9" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="WsMinDist" Value="{Binding Path=WsMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="9" Maximum="60" Minimum="1" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>

                            <TextBox Text="{Binding ElementName=WsMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="9" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="WsMaxDist" Value="{Binding Path=WsMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="9" Maximum="120" Minimum="{Binding ElementName=WsMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="FrostBlink:" IsChecked="{Binding Path=EnableFbValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="10" VerticalAlignment="Top" ToolTip="Use FrostBlink?"></CheckBox>
                            <TextBox Text="{Binding ElementName=FbMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="10" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="FbMinDist" Value="{Binding Path=FbMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="10" Maximum="60" Minimum="33" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>

                            <TextBox Text="{Binding ElementName=FbMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="10" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="FbMaxDist" Value="{Binding Path=FbMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="10" Maximum="120" Minimum="{Binding ElementName=FbMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="Dash:" IsChecked="{Binding Path=EnableDsValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="11" VerticalAlignment="Top" ToolTip="Use Dash?"></CheckBox>
                            <TextBox Text="{Binding ElementName=DsMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="11" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="DsMinDist" Value="{Binding Path=DsMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="11" Maximum="60" Minimum="33" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>

                            <TextBox Text="{Binding ElementName=DsMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="11" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="DsMaxDist" Value="{Binding Path=DsMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="11" Maximum="120" Minimum="{Binding ElementName=DsMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                            <CheckBox Content="Bodyswap:" IsChecked="{Binding Path=EnableBsValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="10,10,0,0" Grid.Row="12" VerticalAlignment="Top" ToolTip="Use Bodyswap?"></CheckBox>
                            <TextBox Text="{Binding ElementName=BsMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="12" Margin="210,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="BsMinDist" Value="{Binding Path=BsMinDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="120,10,0,0" Grid.Row="12" Maximum="60" Minimum="33" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Min distance from destination to trigger this skill."></Slider>

                            <TextBox Text="{Binding ElementName=BsMaxDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" Grid.Row="12" Margin="340,10,0,0" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True"></TextBox>
                            <Slider x:Name="BsMaxDist" Value="{Binding Path=BsMaxDistValue, Mode=TwoWay}" HorizontalAlignment="Left" Margin="250,10,0,0" Grid.Row="12" Maximum="120" Minimum="{Binding ElementName=BsMinDist, Path=Value, UpdateSourceTrigger=PropertyChanged}" TickPlacement="BottomRight" TickFrequency="10" Width="80" SmallChange="1" ToolTip="Max distance from destination to trigger this skill."></Slider>

                        </Grid>
                    </StackPanel>
                </Expander>
                <Expander Header="Advice">
                    <StackPanel>
                        <Label Content="NEWER set default movement skill OR skill to move to mouse buttons, ALWAYS us q,w,e,r,t for them."></Label>
                        <Label Content="If you do not know how to configure the plugin, leave the settings to default values."></Label>
                    </StackPanel>
                </Expander>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
